<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Part12_ResourceSystem.Views.Priority">
    <UserControl.Resources>
        <x:String x:Key="btnc">UserControlResource</x:String>
    </UserControl.Resources>
    <UserControl.Styles>
        <Style>
            <Style.Resources>
                <x:String x:Key="btnc">UserControlStyleResource</x:String>
            </Style.Resources>
        </Style>
    </UserControl.Styles>
    <StackPanel>
        <StackPanel.Resources>
            <x:String x:Key="btnc">StackPanelResource</x:String>
        </StackPanel.Resources>
        <StackPanel.Styles>
            <Style>
                <Style.Resources>
                    <x:String x:Key="btnc">StackPanelStyleResource</x:String>
                </Style.Resources>
            </Style>
        </StackPanel.Styles>
        <Button
            FontSize="50"
            FontWeight="Bold">
            <Button.Resources>
                <x:String x:Key="btnc">ButtonResource</x:String>
            </Button.Resources>
            <Button.Styles>
                <Style>
                    <Style.Resources>
                        <x:String x:Key="btnc">ButtonStyleResource</x:String>
                    </Style.Resources>
                </Style>
            </Button.Styles>
            <Button.Content>
                <StaticResource ResourceKey="btnc" />
            </Button.Content>
        </Button>
    </StackPanel>
</UserControl>